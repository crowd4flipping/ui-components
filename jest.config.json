{
  "testEnvironment": "jsdom",
  "clearMocks": true,
  "coverageProvider": "v8",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "testMatch": [
    "**/__tests__/**/*.[jt]s?(x)",
    "**/?(*.)+(spec|test).[tj]s?(x)"
  ],
  "transform": {
    "^.+\\.tsx?$": ["ts-jest", {}]
  },
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy"
  }
}
